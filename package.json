{
  "name": "discord-bot",
  "version": "1.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"*.{js,ts}\" \"{**,.}/*.{js,ts}\"",
    "start": "TS_NODE_PROJECT=tsconfig.node.json node -r tsconfig-paths/register lib/index.js",
    "start:dev": "nodemon",
    "start:dev-w": "nodemon -L",
    "test": "exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.6.0",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "chartjs-node-canvas": "^3.0.6",
    "config": "^3.3.3",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ramda": "^0.27.1",
    "string-argv": "^0.3.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "uuid": "^8.3.2",
    "yargs": "^17.0.0-candidate.0"
  },
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/ramda": "^0.27.39",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.22.1",
    "depcheck": "^1.3.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "sort-package-json": "^1.50.0",
    "typescript": "^4.2.3"
  }
}
